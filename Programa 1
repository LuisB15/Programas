#include <stdio.h>
#include <stdlib.h>


int main(int argc, char**argv){
	long long a, b;

	// Exit if no arguments are provided
	if (argc < 3) return -1;
	// Read a from argument 1, converting string to integer
	a = (long long)strtol(argv[1], NULL, 10);
	// Read b from argument 2, converting string to integer
	b = (long long)strtol(argv[2], NULL, 10);

	// Print result
	long long c = a + b;
	printf("%lld\n",c);

	return 0;
}
